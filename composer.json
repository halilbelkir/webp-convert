{
  "name": "halilbelkir/img-webp-convert",
  "description": "Laravel'de çalışan HTML etiketi olan 'img' etiketinin oluşturulması ve jpg,png vs. resim formatlarının webp formatına dönüştüren bir kütüphanedir. ",
  "license": "MIT",
  "authors": [
    {
      "name": "Halil İbrahim BELKIR",
      "email": "ibrahim@halilbelkir.com",
      "homepage": "https://www.halilbelkir.com",
      "role" : "Kıdemli Yazılım Uzmanı"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "halilBelkir\\WebConvert\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "henzeb/composer-link": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "halilBelkir\\WebConvert\\WebpConvertServiceProvider"
      ]
    }
  }
}
